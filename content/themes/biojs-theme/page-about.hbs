          <div class="col s12">
            <h2 class="light">{{title}}<i class="material-icons biojs-pink-text left">{{meta_description}}</i></h2>
            {{content}}
            <hr class="mt-2" />
          </div>
        </div>
          {{/foreach}}
          {{/get}}
      </div>
    </section>
