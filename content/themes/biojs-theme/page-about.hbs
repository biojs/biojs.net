{{!< default}}
<header class="biojs-light-green biojs-white-text">
  <div class="container">
  <div class="row">
     <div class="col s12 mt-1">
       {{#get "posts" filter="title:about-header"}}
          {{#foreach posts}}
              {{content}}
          {{/foreach}}
       {{/get}}
     </div>
  </div>
</header>
