{{!< default}}

{{! This is a page template. A page outputs content just like any other post, and has all the same
    attributes by default, but you can also customise it to behave differently if you prefer. }}


{{#post}}
{{#has tag="brandpage"}}
{{! Everything inside the #post tags pulls data from the page }}

    <div class="biojs-green biojs-white-text">
      <div class="container">
       {{#get "posts" slug="starters-header"}}
        {{#foreach posts}}
            <div class="row">
              <div class="col s12">
                <h1 class="brand-title light">{{ title }}</h1>
              </div>
            </div>
            <div class="row pb-2 mb-0">
              <div class="col s12 m7">
                    {{ content }} <br/> 
              </div>        
              <div class="col s12 m5">
                <p><a class="waves-effect waves-blue btn-large action uppercase" href="#"><i class="material-icons left">play_arrow</i>Get Started</a></p>
              </div>
            </div>
          </div>
        {{/foreach}}
      {{/get}}
    </div>
    <section class="container">
      {{> bene }}
    </section>
    <section>
      {{> example}}
    </section>
    <section class="container">
      <div class="row">
        <div class="col s12">
          {{ content }}

        </div>
      </div>
      <hr />
      <div class="row">
         {{> tip-section }}
      </div>
    </section>
    <section class="biojs-black biojs-white-text center">
      {{> showcase}}
    </section>
    <section>
      <div class="container">
        <div class="row">
          <div class="col s12">
            <h2>Lorem ipsum lore CTA section title</h2>
            {{#get "posts" slug="starters-bottom"}}
              {{#foreach posts}}
                {{ content }}
              {{/foreach}}
            {{/get}}
            <p class="center mt-4 mb-2"><a href="http://edu.biojs.net/101/intro/" class="waves-effect waves-yellow action btn-large">GET STARTED</a></p>
          </div>
        </div>
      </div>
    </section>
    {{/has}}
{{/post}}




