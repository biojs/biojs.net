<div class="container">
  <div class="row">
    <div class="col s12">
    {{#has tag="brandpage-starters"}}
      {{#get "posts" slug="starters-showcase"}}
        {{#foreach posts}}
        <h2>{{title}}</h2>
        <p>{{ content }}</p>
        {{/foreach}}
      {{/get}}
      <div class="row">
        {{#get "posts" filter="tags:showcase"}}
          {{#foreach posts}}
          <div class="col s12 m4">
            <div class="card left-align">
                <div class="card-image waves-effect waves-block waves-light">
                  <img class="activator" src="{{ image }}">
                </div>
                <div class="card-content">
                  <span class="card-title activator grey-text text-darken-4">{{title}}<i class="material-icons right">more_vert</i></span>
                  <p><a href="{{ meta_description }}" target={{slug}}>Link<i class="material-icons link-icon tiny mv-1">open_in_new</i></a></p>
                </div>
                <div class="card-reveal black-text">
                  <span class="card-title grey-text text-darken-4">{{title}}<i class="material-icons right">close</i></span>
                  <p>{{ content }}</p>
                </div>
              </div>
            </div>
          {{/foreach}}
        {{/get}}
    </div>
    {{/has}}
    </div>
  </div>
</div>




